<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
    and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
    {#{{ encore_entry_link_tags('app') }}#}
    <link href="{{ asset('/css/style.css') }}" rel="stylesheet">
    {% endblock %}
</head>

<body>
    {% if (app.user) %}
    <header>{% block header %}
        <nav>
            <div class="navWrapper">
                <a href="">
                    <div id="instaNav"></div>
                </a>
                <input type="text" placeholder="Rechercher">
                <div class="iconMenu">
                    <a href="">
                        <div id="home"></div>
                    </a>
                    <a href="">
                        <div id="chat"></div>
                    </a>
                    <a href="">
                        <div id="compass"></div>
                    </a>
                    <a href="">
                        <div id="like"></div>
                    </a>
                    <div id="panel"></div>
                </div>
            </div>
        </nav>
        {% endblock %}
    </header>
    {% endif %}


    {% block body %}{% endblock %}
    {% block javascripts %}
    <script src="{{ asset('/js/main.js') }}"></script>
    {% endblock %}
</body>

</html>